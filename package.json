{
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=20.x",
        "npm": ">=10.2.x"
    },
    "os": [
        "!darwin"
    ],
    "dependencies": {
        "alpinejs": "^3.13.5",
        "jquery": "^3.7.1",
        "jquery-ui": "^1.13.2",
        "jscroll": "^2.4.1",
        "jsdom": "^24.0.0",
        "swiper": "^11.0.5"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/typography": "^0.5.0",
        "autoprefixer": "^10.4.7",
        "axios": "^1.6.4",
        "chokidar-cli": "^3.0.0",
        "depcheck": "^1.4.7",
        "laravel-vite-plugin": "^1.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4",
        "sass": "^1.69.6",
        "tailwindcss": "^3.1.0",
        "vite": "^5.0.0"
    },
    "scripts": {
        "sass:compile": "sass -q -s compressed --source-map ./public/sass:./public/css",
        "sass:write": "sass --watch -q -s compressed --source-map ./public/sass:./public/css --update",
        "dev": "vite",
        "prebuild": "npm run sass:compile",
        "build": "vite build",
        "start": "php artisan serv --port=80 --host=0.0.0.0"
    }
}
